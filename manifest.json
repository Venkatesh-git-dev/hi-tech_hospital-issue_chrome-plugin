{
  "manifest_version": 3,
  "name": "Btn1",
  "version": "1.0",
  "description": "Creates a button to click the element with id 'abc' on the page",
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"]
    }
  ],
  "action": {
    "default_popup": "popup.html"
  }
}
